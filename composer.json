{
  "name": "rhubarbphp/scaffold-application-versions",
  "description": "Provides scripted updates for managed transitions in application versions",
  "keywords": [ "version", "script", "rhubarb", "php", "framework" ],
  "homepage": "http://www.rhubarbphp.com/",
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "Rhubarb\\Scaffolds\\ApplicationVersions\\": "src/",
      "Rhubarb\\Scaffolds\\ApplicationVersions\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/acuthbert/rhubarb"
    }
  ],
  "require": {
    "rhubarbphp/rhubarb": "dev-crown",
    "rhubarbphp/module-stem": "dev-migration",
    "rhubarbphp/scaffold-application-settings": "dev-feature/FirstVersion"
  },
  "require-dev": {
    "phpunit/phpunit": "3.*"
  }
}

